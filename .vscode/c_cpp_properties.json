{
    "configurations": [
        {
            "name": "RISC-V OS",
            "compilerPath": "/usr/bin/riscv64-unknown-elf-gcc",
            "includePath": [
                "${workspaceFolder}/include",
                "${workspaceFolder}/**"
            ],
            "cStandard": "c11",
            "intelliSenseMode": "${default}",
            "compilerArgs": [
                "-nostdlib",
                "-fno-builtin",
                "-march=rv64gc",
                "-mabi=lp64d",
                "-mcmodel=medany"
            ]
        }
    ],
    "version": 4
}
